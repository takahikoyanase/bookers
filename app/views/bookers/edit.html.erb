<h1>Editing Book</h1>
<% if @booker.errors.any? %>
 <%= @booker.errors.count %>件のエラーが発生しました。
 <% @booker.errors.full_messages.each do |message| %>
  <%= message %>
 <% end %>
<% end %>

<%= form_with model:@booker, local: true do |f| %>
<h4>Title</h4>
<%= f.text_field :title %>
<h4>Body</h4>
<%= f.text_area :body %> |
<%= f.submit 'Update Book' %>
<% end %>
<p>
<%= link_to "show", booker_path(booker.id) %> |
<%= link_to "back", :back%>
</p>